{
  "name": "nullodyssey/circuit-breaker",
  "description": "PHP Circuit Breaker implementation",
  "minimum-stability": "stable",
  "version": "0.0.1",
  "license": "MIT",
  "authors": [
    {
      "name": "NullOdyssey",
      "email": "community@nullodyssey.dev"
    }
  ],
  "require": {
    "php": "^8.3"
  },
  "autoload": {
    "psr-4": {
      "NullOdyssey\\CircuitBreaker\\": "src/"
    }
  },
    "autoload-dev": {
        "psr-4": {
          "NullOdyssey\\CircuitBreaker\\Tests\\": "tests/"
        }
    },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.75",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^12.2",
    "phpstan/phpstan-strict-rules": "^2.0",
    "phpstan/phpstan-deprecation-rules": "^2.0",
    "phpstan/phpstan-phpunit": "^2.0",
    "ekino/phpstan-banned-code": "^3.0"
  }
}
